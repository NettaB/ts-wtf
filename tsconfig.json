{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "Store": ["src/store"],
      "Store/*": [
        "src/store/*"
      ],
      "Util/*": [
        "src/util/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Components": [
        "src/components"
      ],
      "Types/*": [
        "src/types/*"
      ],
      "Assets/*": [
        "assets/*"
      ]
    }
  }
}
